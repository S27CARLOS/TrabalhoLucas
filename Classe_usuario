package Entities;

public class Usuario {
    private String nome;
    private String cpf;
    private String email;
    private int idade;
    private boolean admin;

    public Usuario() {
    }

    public Usuario(String nome, int idade, String cpf, String email, boolean admin) {
        this.nome = nome;
        this.idade = idade;
        this.cpf = cpf;
        this.email = email;
        this.admin = admin;
    }

    public String getNome() {
        return nome;
    }

    public int getIdade() {
        return idade;
    }

    public String getCpf() {
        return cpf;
    }

    public String getEmail() {
        return email;
    }

    public boolean Admin() {
        return admin;
    }
}
